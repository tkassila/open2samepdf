          <Button fx:id="buttonOpen" minHeight="-Infinity" minWidth="-Infinity" onAction="#buttonOpenPressed" prefHeight="25.0" prefWidth="158.0" text="Open pdf 2 times" HBox.hgrow="ALWAYS" />
